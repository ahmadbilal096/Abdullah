interface Animal {
  public void animalSound(); 
  public void sleep();
}
class Pig implements Animal {
  public static void main(String[] args) {
	 public void animalSound() {
    System.out.println("The pig says: wee wee");
  }
  public void sleep() {
    System.out.println("Zzz");
  }
    Pig myPig = new Pig();
    myPig.animalSound();
    myPig.sleep();
 
}
